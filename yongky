        //int hasil  = input.nextLine();  // Read user input
        System.out.print("Hasil penjumlahan Angka 1 + Angka 2 =" +hasil + "\n"+ "Dikerjakan Oleh Yongky Bayu Prasetyo");  // Output user input
      
